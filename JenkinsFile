
pipeline {
  agent none
  stages {
    stage('Install node') {
        agent {
        docker { image 'node:16-alpine' }
        }
      steps {
        sh 'node --version'
        sh 'npm i'
      }
    }
  //   stage('Install Dependencies') {
  // steps {
  //   sh 'npm i'
  // }
  //   }
  }
}

